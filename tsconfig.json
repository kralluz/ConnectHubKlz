{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/api/clients.js", "pages/api/contacts/[contactId].ts", "pages/api/contacts.st", "pages/_app.js", "pages/api/session.ts", "pages/api/src/middlewares/handleErrors.middleware.js"  ],
  "exclude": [
    "node_modules"
  ]
}
